{
    "name": "oxygen/core-views",
    "description": "View Files and Rendering Code for the Core",
    "license": "MIT",
    "keywords": [
        "cms",
        "framework",
        "OxygenExtension"
    ],
    "authors": [
        {
            "name": "Chris Chamberlain",
            "email": "c.chamberlain@iinet.net.au"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "oxygen/core": "dev-master",
        "illuminate/support": "4.2.*"
    },
    "autoload": {
        "psr-4": {
            "Oxygen\\CoreViews\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "title": "Oxygen Core Views",
        "readme": "oxygen.md",
        "changes": "changes.md",
        "icon": "icon.png",
        "images": [
            "https://raw.githubusercontent.com/oxygen-cms/core-views/master/docs/buttons.png",
            "https://raw.githubusercontent.com/oxygen-cms/core-views/master/docs/code-view.png",
            "https://raw.githubusercontent.com/oxygen-cms/core-views/master/docs/design-view.png"
        ],
        "providers": [
            {
                "name": "Core Views",
                "class": "Oxygen\\CoreViews\\CoreViewsServiceProvider",
                "description": "Look and Feel of the Administration Section"
            }
        ]
    }
}
