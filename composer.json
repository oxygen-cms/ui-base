{
    "name": "oxygen/ui-base",
    "description": "View Files and Rendering Code",
    "license": "MIT",
    "keywords": [
        "cms",
        "framework",
        "OxygenExtension"
    ],
    "authors": [
        {
            "name": "Chris Chamberlain",
            "email": "chris@chamberlain.id.au"
        }
    ],
    "require": {
        "php": "^8.1",
        "oxygen/core": "~0.15.0",
        "oxygen/auth": "~0.8.0",
        "illuminate/pagination": "^10.0",
        "illuminate/support": "^10.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Oxygen\\UiBase\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "8.1.0"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/oxygen-cms/laraguard-doctrine"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "phpstan/phpstan": "^1.8"
    }
}
