{
    "name": "oxygen/ui-base",
    "description": "View Files and Rendering Code",
    "license": "MIT",
    "keywords": [
        "cms",
        "framework",
        "OxygenExtension"
    ],
    "authors": [
        {
            "name": "Chris Chamberlain",
            "email": "chris@chamberlain.id.au"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "oxygen/core": "~0.8.0",
        "oxygen/auth": "~0.5.2",
        "illuminate/pagination": "~6.0",
        "illuminate/support": "~6.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Oxygen\\UiBase\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "title": "Oxygen - UI Base",
        "readme": "oxygen.md",
        "changes": "changes.md",
        "icon": "icon.png",
        "images": [
            "https://raw.githubusercontent.com/oxygen-cms/ui-base/master/docs/buttons.png",
            "https://raw.githubusercontent.com/oxygen-cms/ui-base/master/docs/code-view.png",
            "https://raw.githubusercontent.com/oxygen-cms/ui-base/master/docs/design-view.png"
        ],
        "providers": [
            {
                "name": "Core Views",
                "class": "Oxygen\\CoreViews\\CoreViewsServiceProvider",
                "description": "Look and Feel of the Administration Section"
            }
        ]
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.18"
    }
}
